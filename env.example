# Supabase Configuration
# Get these values from your Supabase project dashboard
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key

# App Configuration
APP_NAME=Mental Health Support
APP_VERSION=1.0.0

# Feature Flags
